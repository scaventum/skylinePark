  	<description></description>
  	<servlet-class>dbAccess.commonFunction</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>payAction</servlet-name>
  	<url-pattern>/payAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>indexAction</servlet-name>
  	<url-pattern>/indexAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>refundAction</servlet-name>
  	<url-pattern>/refundAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
